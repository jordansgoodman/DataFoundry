# Example resource limits for production-like tuning
services:
  postgres:
    mem_limit: 4g
    cpus: 2.0
  airflow-webserver:
    mem_limit: 2g
    cpus: 1.0
  airflow-scheduler:
    mem_limit: 2g
    cpus: 1.0
  superset:
    mem_limit: 2g
    cpus: 1.0
  pgadmin:
    mem_limit: 512m
    cpus: 0.5
